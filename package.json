{
  "name": "@corets/tag",
  "description": "Type tagging and branding for TypeScript",
  "version": "0.1.0-0",
  "author": "Maxim Kott",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "corets",
    "tags",
    "branding",
    "types"
  ],
  "files": [
    "/dist"
  ],
  "repository": "github:corets/tag",
  "homepage": "https://corets.github.io/tag",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && echo '\nOpen \"coverage/lcov-report/index.html\" in your browser\n'",
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "rm -rf dist && rollup -c",
    "release": "np --no-release-draft --tag next",
    "preview": "np --no-release-draft --tag dev",
    "prepare": "yarn run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "esbuild": "^0.12.15",
    "jest": "^27.0.6",
    "np": "^7.2.0",
    "prettier": "2.2.1",
    "rollup": "^2.52.8",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-esbuild": "^4.5.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
